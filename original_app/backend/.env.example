# Database connection string (e.g., PostgreSQL, MongoDB, etc.)
DATABASE=your_database_connection_string

# Resend API key (used for sending emails)
RESEND_API="your resend_api"

# OpenAI API key
OPENAI_API_KEY="your_openai_api_key"

# Secret key used for signing JWT tokens
JWT_SECRET="your_private_jwt_secret_key"

# Environment mode: development | production | test
NODE_ENV=production

# Suppress OpenSSL configuration errors (commonly used in Node environments)
OPENSSL_CONF=/dev/null

# Public server file URL (used for serving static files or APIs)
PUBLIC_SERVER_FILE=http://localhost:8888/
